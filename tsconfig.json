{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "Preserve",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noEmit": true,
		"moduleDetection": "force",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"outDir": "dist",
	},
}
